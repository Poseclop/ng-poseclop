{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "module": "es2020",
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "ng-cognito": [
        "dist/ng-cognito/ng-cognito",
        "dist/ng-cognito"
      ],
      "ng-typed-form": [
        "dist/ng-typed-form/ng-typed-form",
        "dist/ng-typed-form"
      ],
      "ng-get-pipe": [
        "dist/ng-get-pipe/ng-get-pipe",
        "dist/ng-get-pipe"
      ],
      "ng-responsive-ui": [
        "dist/ng-responsive-ui/ng-responsive-ui",
        "dist/ng-responsive-ui"
      ],
      "ng-one-pager": [
        "dist/ng-one-pager/ng-one-pager",
        "dist/ng-one-pager"
      ],
      "ng-logger-service": [
        "dist/ng-logger-service/ng-logger-service",
        "dist/ng-logger-service"
      ]
    }
  },
  "angularCompilerOptions": {
    "strictInjectionParameters": true,
    "strictInputAccessModifiers": true,
    "strictTemplates": true
  }
}